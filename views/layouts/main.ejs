                transition: transform 0.2s ease, box-shadow 0.2s ease;
            }
            .floating-whatsapp:hover {
                color: #fff;
                transform: translateY(-2px) scale(1.03);
                box-shadow: 0 14px 30px rgba(18, 140, 126, 0.48);
            }
            .floating-whatsapp i {
                font-size: 30px;
                line-height: 1;
            }
            .floating-whatsapp-label {
                position: fixed;
                left: 86px;
                bottom: 38px;
                z-index: 2199;
                background: #0f3460;
                color: #fff;
                font-size: 13px;
                padding: 6px 10px;
                border-radius: 6px;
                opacity: 0;
                transform: translateX(-8px);
                pointer-events: none;
                transition: opacity 0.2s ease, transform 0.2s ease;
            }
            .floating-whatsapp:hover + .floating-whatsapp-label {
                opacity: 1;
                transform: translateX(0);
            }
            @media (max-width: 767px) {
                .floating-whatsapp {
                    left: 14px;
                    bottom: 16px;
                    width: 54px;
                    height: 54px;
                }
                .floating-whatsapp-label {
